            "lg": 6
        },
        "applicationSecret": {
            "newLine": true,
            "type": "password",
            "label": "applicationSecret",
            "help": "applicationSecretHint",
            "validator": "data.applicationSecret.match(/[a-zA-Z0-9]{8}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{12}/)",
            "validatorNoSaveOnError": true,
            "visible": true,
            "repeat": false,
            "sm": 12,
            "md": 6,
            "lg": 6
